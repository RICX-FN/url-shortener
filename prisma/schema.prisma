generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Link {
  id          Int      @id @default(autoincrement())
  originalUrl String   @unique
  shortCode   String   @unique 
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
